@page
@model Avanze_ProjectoWeb.Pages.Projecto.editProjectListaModel
@{
    @if (@CrowdFundingDAO.Model.SessionClass.SessionRole != "")
    {
        Layout = "_LayoutLogin";
    }
    else
    {
        Layout = "_Layout";
    }
}

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/tablas.css" asp-append-version="true" />
</head>

<body>
    <h2>Lista de Proyectos</h2>
    <br>

    <table class="styled-table">
        <thead>
            <tr>
              
                <th> Nombre de Projecto </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var prod in Model.ProjectsLista)
            {
                <tr>
                    
                    <td>@prod.title</td>

                    <td>
                        <a class="btn1" asp-page="editProject" asp-route-id="@prod.id"> Edit</a>

                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>

